{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-class-properties",
    [
      "react-intl-auto",
      {
        "removePrefix": "src/",
        "filebase": false
      }
    ],
    [
      "react-intl",
      {
        "messagesDir": "./src/UI/i18n/messages"
      }
    ]
  ]
}
